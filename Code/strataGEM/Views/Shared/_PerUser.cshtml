@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    string b = User.Identity.GetUserId();
    if (b == (string)ViewData["n"])
    {
        <p>Eliminar</p>
        <a href=@Url.Action("Editar", "Review", new { id = Model.Review_Id})>Editar</a>
    }
}
else
{
    <p>Registrate para cualquier cosa</p>
}
